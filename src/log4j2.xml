<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<Appenders>
		<File name="File" fileName="${sys:user.home}/ShareS/logs/run.log">
      		<PatternLayout>
        		<Pattern>%d %p [%c{1.}] - %m%n</Pattern>
      		</PatternLayout>
		</File>
	</Appenders>
	
	<Loggers>
		<Root level="ALL">
			<AppenderRef ref="File"/>
		</Root>
	</Loggers>
</Configuration>